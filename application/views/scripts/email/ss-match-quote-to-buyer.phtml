/**
 * Prints out table row with quote details
 *
 * @param   array       $row
 * @param   Zend_View   $view
 */
$echoQuoteRow = function(array $row, Zend_View $view, $buyerSelectedHasQuoted100Percent = false) {
    $isMatch = in_array($row['QOT_INTERNAL_REF_NO'], $view->matchQuoteIds);
    $isThis = $row['QOT_INTERNAL_REF_NO'] == $view->quote->qotInternalRefNo;

    // the quote which email is about has its own row style
    // style of other rows depends on whether they are match quotes or not

    $tdStyle = implode('; ', array(
        "font-family: Arial, Helvetica, Sans-Serif",
        "font-size: 11px",
        "padding: 9px",
        "border-right:  1px solid #bec4cb",
        "border-bottom: 1px solid #bec4cb",
        "color: #2e3235"
    ));

    if ($isThis) {
        $bgColour = '#83cc4e';
    } else if ($isMatch) {
        $bgColour = '#e5f8d8';
    } else {
        $bgColour = "#ffffff";
    }

    $tdStyle .= implode('; ', array(
        "background-color: " . $bgColour,
        ""
    ));
    ?>

    <tr bgcolor="<?= $bgColour; ?>" style="background-color: <?php echo $bgColour; ?>">
        <td bgcolor="<?= $bgColour; ?>" style="border-left: 1px solid #bec4cb; <?= $tdStyle; ?>; font-weight: bold;">
            <?php
            if ($isThis) {
                ?>This quote<br/><?php
            }
            ?><a href="<?= $row['QUOTE_URL'] ?>"><?php
            if ($isMatch) {
                ?><?= $this->quoteTypeLabel ; ?> selected<?php
            } else {
                ?>Buyer selected<?php
            }
            ?></a>
        </td>

        <td bgcolor="<?= $bgColour; ?>" style="<?= $tdStyle; ?>">
            <?= $row['QUOTEDATE']->format('j M Y'); ?>
            <!-- Quote ID:     <?= $row['QOT_INTERNAL_REF_NO']; ?> -->
            <!-- Quote RFQ ID: <?= $row['QOT_RFQ_INTERNAL_REF_NO']; ?> -->
        </td>

        <td bgcolor="<?= $bgColour; ?>" style="<?php echo $tdStyle; ?>">
            <b><?= htmlentities($row['SPB_NAME']); ?></b>
            <br/>
            <?php
            $statusCellStr = array();

            // check if the supplier is whitelisted by the buyer from the quote
            if ($row['WHITELISTED'] and !$row['WHITELISTDISABLED']) {
                $statusCellStr[] = "This supplier is on your approved vendor list.";
            }

            // check the last order date for the buyer from the quote
            $lastOrderDate = $row['LASTORDERDATE'];
            if (is_null($lastOrderDate)) {
                $statusCellStr[] = "You have never used this supplier before.";
            } else {
                $statusCellStr[] = "You last ordered from this supplier on " . $lastOrderDate->format('j M Y') . ".";
            }

            echo implode(" ", $statusCellStr);
            ?>

        </td>

        <td bgcolor="<?= $bgColour; ?>" style="<?php echo $tdStyle; ?>">
            <?= htmlentities($row['QUOTE_GENUINE_MESSAGE']); ?>
            <?php
                /* switch ($row[Shipserv_Quote::COL_GENUINE]) {
                case Shipserv_Quote::GENUINE_YES:
                    echo 'Yes';
                    break;

                case Shipserv_Quote::GENUINE_NO:
                    echo 'No';
                    break;

                case Shipserv_Quote::GENUINE_NA:
                    echo 'Not Applicable';
                    break;

                case Shipserv_Quote::GENUINE_UNKNOWN:
                default:
                    echo 'Unknown';
                } */ ?>
        </td>

        <td bgcolor="<?= $bgColour; ?>" style="<?php echo $tdStyle; ?>">
            <?= htmlentities(Myshipserv_View_Helper_String::truncateToWord($row[Shipserv_Quote::COL_PAYMENT_TERMS], 100)); ?>
        </td>

        <td bgcolor="<?= $bgColour; ?>" style="<?php echo $tdStyle; ?>">
            <?= htmlentities(Shipserv_Quote::getReadableDeliveryTermsForKey($row[Shipserv_Quote::COL_DELIVERY_TERMS])); ?>
