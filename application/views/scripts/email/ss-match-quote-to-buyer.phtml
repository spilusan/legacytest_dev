                    {
                        echo number_format($ps, 2) . " USD";
                    }
                    else if( $ps == null )
                    {
                        if( $row['NORMALISEDTOTAL'] == 0 )
                        {
                            echo "None";
                        }
                        else
                        {
                            echo "No comparable quotes";
                        }

                    }
                    else if( $ps <= 0 )
                    {
                        if( $buyerSelectedHasQuoted100Percent == false )
                        {
                            echo "No comparable quotes";
                        }
                        else
                        {
                            echo 'None';
                        }
                    }
                }
            ?>
        </td>
    </tr>
    <?php
}
