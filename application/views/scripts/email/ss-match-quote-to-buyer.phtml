        </td>

        <td bgcolor="<?= $bgColour ?>" style="<?= $tdStyle ?>;text-align: right;">
            <?= number_format($row['QOT_TOTAL_COST'], 2); ?> <?= $row['QOT_CURRENCY'] ?>
        </td>

        <td bgcolor="<?= $bgColour ?>" style="<?php echo $tdStyle ?>;text-align: right;">
            <?= number_format($row['NORMALISEDTOTAL'], 2) ?> USD
            <?
                if( $row['CHEAPEST_RANK'] == 1){
                    ?><span style="color: red">Cheapest</span><?
                }
            ?>
        </td>

        <td bgcolor="<?= $bgColour ?>" style="<?php echo $tdStyle ?>;text-align: right;">
            <?
                if( $isMatch )
                {
                    $ps = $this->matchQuotePotentialSaving[$row['QOT_SPB_BRANCH_CODE'] . '-' . $row['QOT_INTERNAL_REF_NO']];
