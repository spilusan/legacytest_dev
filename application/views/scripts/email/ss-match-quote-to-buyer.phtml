        <td bgcolor="<?= $bgColour; ?>" style="<?php echo $tdStyle; ?>">
            <?= htmlentities($row[Shipserv_Quote::COL_LEAD_TIME]); ?>
        </td>

        <td bgcolor="<?= $bgColour ?>" style="<?= $tdStyle ?>; text-align: center; font-weight: bold;">
