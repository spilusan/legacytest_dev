<script id="tableTemplate" type="text/x-jsrender">
		<table {{if id}}id="{{>id}}"{{/if}} class="stats {{if displayLeftColumn}}titlecol{{/if}} {{if cssClass}}{{>cssClass}}{{/if}}">
			<thead>
				{{if title}}
				<tr>
					{{if displayLeftColumn}}
						<th class="titlecol empty"></th>
					{{/if}}
					<th class="tbltitle" {{* ret += '';
						var x =1;
						if (view.data.periods.period2) { x = 3; }
						if (view.data.vars) { x = x*2; }
						if (x>0) { ret+= ' colspan="'+ x +'" '; }
						}}>{{>title}}</th>
				</tr>
				{{/if}}
				<tr class="subtitle">
					{{if displayLeftColumn}}
						{{if vars}}
							<th class="titlecol empty"></th>
						{{else}}
							<th class="titlecol"><span class="nowrap">{{>leftColumnTitle}}</span></th>
						{{/if}}
					{{/if}}
					<th class="subtitle period one" {{if vars}}colspan="2"{{/if}}>{{formatDate periods.period1.from /}} - {{formatDate periods.period1.to /}}</th>
					{{if periods.period2}} 
						<th class="subtitle period two" {{if vars}}colspan="2"{{/if}}>{{formatDate periods.period2.from /}} - {{formatDate periods.period2.to /}}</th>
						<th class="subtitle change" {{if vars}}colspan="2"{{/if}}>% Period Change</th>
					{{/if}}
				</tr>
				{{if vars}}
					<tr class="coltitle">
						{{if displayLeftColumn}}
							<th class="titlecol"><span class="nowrap">{{>leftColumnTitle}}</span></th>
						{{/if}}
						<th>{{>vars.var1}}</th>
						<th>{{>vars.var2}}</th>
						{{if periods.period2}}
							<th>{{>vars.var1}}</th>
							<th>{{>vars.var2}}</th>
							<th>{{>vars.var1}}</th>
							<th>{{>vars.var2}}</th>
						{{/if}}
					</tr>
				{{/if}}
			</thead>
			<tbody>
				{{for data}}
					<tr>
						{{if #view.parent.parent.data.displayLeftColumn}}
							<td class="titlecol" title="{{:name}}"><span class="nowrap">{{:name}}</span></td>
						{{/if}}
						<td>{{:period1.var1}}</td>
						{{if period1.var2}}
							<td>{{:period1.var2}}</td>
						{{/if}}
						{{if period2}}
							{{if period2.var1}}
								<td>{{:period2.var1}}</td>
							{{/if}}
							{{if period2.var2}}
								<td>{{:period2.var2}}</td>
							{{/if}}
							{{if period2.var1}}
								<td>{{*
									if (parseFloat(view.data.period1.var1)!=0 && parseFloat(view.data.period2.var1)!=0)
									{
										var val = Math.round(parseFloat(view.data.period1.var1)/parseFloat(view.data.period2.var1)*100 - 100);
										ret+= '<span class="';
										ret+= val > 0 ? 'pos">+' : (val == 0 ? 'zero">' : 'neg">');
										ret+= val +"%</span>";
									}
								}}</td>
							{{/if}}
							{{if period2.var2}}
								<td>{{*
									if (parseFloat(view.data.period1.var2)!=0 && parseFloat(view.data.period2.var2)!=0)
									{
										var val = Math.round(parseFloat(view.data.period1.var2)/parseFloat(view.data.period2.var2)*100 - 100);
										ret+= '<span class="';
										ret+= val > 0 ? 'pos">+' : (val == 0 ? 'zero">' : 'neg">');
										ret+= val +"%</span>";
									}
								}}</td>
							{{/if}}
						{{/if}}
					</tr>	
				{{/for}}
			</tbody>
		</table>
</script>
